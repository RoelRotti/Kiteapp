<knowledge>
	<title>Gear choice</title>
	<description>What kind of gear can you use?</description>




<!-- #################################### RULES #################################### -->
	
	<rule>
		<description>below 8 knots = no kiting</description>
		<if>
			<fact name="wind0-8Knots">yes</fact>					
		</if>
		<then>
			<fact name="kite">no</fact>
			<fact name="windTooSoft">yes</fact>
			<fact name="twintip">no</fact>
		</then>
	</rule>	
		
	<rule>
		<description>8-12 knots and above 70 kg = no kiting</description>
		<if>
			<and>
				<fact name="wind8-12Knots">yes</fact>	
				<or>
					<fact name="weight70-90kg">yes</fact>
					<fact name="weightAbove90kg">yes</fact>
				</or>
			</and>
		</if>
		<then>
			<fact name="kite">no</fact>
			<fact name="windTooSoft">yes</fact>
			<fact name="twintip">no</fact>
		</then>
	</rule>	
	
	

	
	<rule>
		<description>between 8-12 knots and below 70kg + waves = 15m2</description>
